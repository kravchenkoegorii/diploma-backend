{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:api": "node ./dist/apps/api/main.js",
    "start:cron": "node --max-old-space-size=8192 ./dist/apps/cron/main.js",
    "start:api:dev": "nest start --watch --sourceRoot ./src/apps/api --entryFile ./apps/api/main",
    "start:cron:dev": "nest start --watch --sourceRoot ./src/apps/cron --entryFile ./apps/cron/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "yarn run build && npx typeorm -d dist/common/database/database.migrations.js",
    "migration:generate": "yarn run typeorm -- migration:generate",
    "migration:create": "npx typeorm migration:create",
    "migration:revert": "yarn run typeorm -- migration:revert",
    "migration:run": "yarn run typeorm -- migration:run",
    "typeorm:prod": "npx typeorm -d dist/common/database/database.migrations.js",
    "migration:run:prod": "yarn run typeorm:prod -- migration:run"
  },
  "dependencies": {
    "@alloralabs/allora-sdk": "^0.1.0",
    "@de-fi/sdk": "^2.2.8",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^11.0.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.0",
    "@privy-io/server-auth": "^1.16.11",
    "@urql/core": "^5.1.0",
    "alchemy-sdk": "^3.5.3",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "bull": "^4.16.5",
    "cache-manager": "^5.5.2",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "fuse.js": "^7.1.0",
    "graphlib": "^2.1.8",
    "graphology": "^0.26.0",
    "graphology-simple-path": "^0.2.0",
    "graphology-types": "^0.24.8",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "openai": "^4.94.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "remove-markdown": "^0.6.0",
    "request-ip": "^3.3.0",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "viem": "^2.23.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.13",
    "@types/graphlib": "^2.1.12",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.17.14",
    "@types/node": "18.11.18",
    "@types/request-ip": "^0.0.40",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
